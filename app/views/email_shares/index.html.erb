<table>
  <h3>Shares</h3>
  <tr>
    <th>email</th>
    <th>status</th>
    <th>pull date</th>
    <th>User</th>
    <th>Use Case</th>
  </tr>
  <% @email_shares.each do |share| %>
    <tr>
      <td><%= share.email %></td>
      <td>?</td>
      <td><%= share.created_at %></td>
      <td><%= share.card.user.email %></td>
      <td><%= share.card.use_case %></td>
    </tr>
  <% end %>
</table>